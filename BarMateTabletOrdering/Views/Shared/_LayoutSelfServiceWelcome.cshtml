<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=no, minimal-ui">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <link rel="apple-touch-icon" href="images/apple-touch-icon.png" />
    <link href="images/apple-touch-startup-image-320x460.png" media="(device-width: 320px)" rel="apple-touch-startup-image">
    <link href="images/apple-touch-startup-image-640x920.png" media="(device-width: 320px) and (-webkit-device-pixel-ratio: 2)" rel="apple-touch-startup-image">
    <title>Cabbash - Bring Your Own Device</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/sscss/framework7.css">
    <link rel="stylesheet" href="~/sscss/style.css">
    <link type="text/css" rel="stylesheet" href="~/sscss/swipebox.css" />
    <link type="text/css" rel="stylesheet" href="~/sscss/animations.css" />
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,900" rel="stylesheet">
    <link href="~/confirm/css/jquery-confirm.css" rel="stylesheet" />


    @*<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/webrtc-adapter/3.3.3/adapter.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/vue/2.1.10/vue.min.js"></script>
    <script type="text/javascript" src="https://rawgit.com/schmich/instascan-builds/master/instascan.min.js"></script>*@


</head>
<body id="mobile_wrap">

    <div class="statusbar-overlay"></div>

    <div class="panel-overlay"></div>

    <div class="panel panel-left panel-reveal">
        <div class="view view-subnav">
            <div class="pages">
                <div data-page="panel-leftmenu" class="page pagepanel">
                    <div class="page-content">
                        <nav class="main_nav_underline">
                            <ul>
                                <li><a href="#" class="close-panel" data-view=".view-main"><img src="~/ssimages/icons/white/home.png" alt="" title="" /><span>Home</span></a></li>
                            </ul>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="panel panel-right panel-reveal">
        <div class="user_login_info">

            <div class="user_thumb">
                <img src="~/ssimages/page_photo.jpg" alt="" title="" />
                <div class="user_details">
                    <p>Welcome, <span></span></p>
                </div>
                <div class="user_avatar"><img src="~/ssimages/avatar.jpg" alt="" title="" /></div>
            </div>

        </div>
    </div>

    @RenderBody()

    <script type="text/javascript" src="~/ssjs/jquery-1.10.1.min.js"></script>
    <script type="text/javascript" src="~/ssjs/framework7.js"></script>
    <script type="text/javascript" src="~/ssjs/jquery.swipebox.js"></script>
    <script type="text/javascript" src="~/ssjs/jquery.fitvids.js"></script>
    <script type="text/javascript" src="~/ssjs/email.js"></script>
    <script type="text/javascript" src="~/ssjs/audio.min.js"></script>
    <script type="text/javascript" src="~/ssjs/my-app.js"></script>
    @*<script src="~/confirm/js/jquery-confirm.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.js"></script>*@
    @RenderSection("scripts", required: false)

</body>
</html>