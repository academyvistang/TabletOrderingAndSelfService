@model BarMateTabletOrdering.Models.CabbashViewModel

@if (Model.CategoryId == 0)
{
    <span class="categories selectedGat" id=""><i class="fa fa-home"></i></span>
}
else
{
    <span class="categories" id=""><i class="fa fa-home"></i></span>
}

@foreach (var cat in Model.CategoriesListString)
{
    <span class="categories" id="@cat">@cat</span>
}
